page{
  background-color: rgb(24, 129, 36);
  width: 100%;
  height: 100%;
}
.set{
  width: 80rpx;
  height: 80rpx;
  margin-left: 20rpx;
  float: left;
}
.shaiziCount{
  width: 222rpx;
  height: 80rpx;
  background-color: #D7D5D4;
  border-radius: 10rpx;/*圆角*/
  text-align: center;
  line-height: 80rpx;
  color: rgb(24, 129, 36);
  font-size: 44rpx;
  float: right;
  margin-right: 20rpx;
}

.reduceBtn{
  width: 80rpx;
  height: 80rpx;
  float: right;
}
.addBtn{
  width: 80rpx;
  height: 80rpx;
  float: left;/*为什么需要设置float*/
}
.footer{
  width: 100%;
  height: 110rpx;
  background-color: #0A4B1F;
  position: absolute;
  left: 0;
  bottom: 0;
}
.shake{
  width: 80rpx;
  height: 80rpx;
  margin-top: 18rpx;
  margin-left: 277rpx;
  float: left;
  /* clear: both; */
/*    
  display: flex;
  flex-direction: column;
  justify-content: center;  */
}
.shakeText{
  clear: both;
  height: 110rpx;
  /* display: inline-block; */
  margin-left: 20rpx;
  color: white;
  font-size: 44rpx;
  line-height: 110rpx;
  display: inline-block;
  }
  .point{
    width: 76px;
    height: 76px;
    position: fixed;
    left: -76px;
    top:-76px;
  }
  .alertBody{
    background-color: #4A4A4A;
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0px;
    top: 0px;
    display: flex;
    flex-direction: row;
    justify-content: center;
  }
  .alertView{
    width: 250px;
    height: 260px;
    margin-top: 100px;
    border-radius: 10px;
    display: flex;
    flex-direction: column;
    align-items: center;
  }
  .close{
    width: 38px;
    height: 38px;
    display: flex;
    align-self: flex-end;
    margin-top: -15px;
    margin-right: -10px;
  }
  .text{
    font-size: 32rpx;
    color: #188124;
    margin-bottom: 52rpx;
  }
  .textContain{
    display: flex;
    flex-direction: column;
    margin-top: 100px;
    align-self: flex-start;
    margin-left: 59px;
  }
  .row_contain{
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
  }
  .gou{
    display: flex;
    align-self: flex-start;
    width: 17px;
    height: 17px;
    margin-left: 75px;
    margin-top: 3px;
  }
  .shakeGou{
    margin-left: 60px;
  }
  .shakeImage{
    width: 27px;
    height: 27px;
    display: flex;
    align-self: flex-start;
    margin-left: -20px;
  }